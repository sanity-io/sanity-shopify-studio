{
  "root": true,
  "api": {
    "projectId": "wfr1r0dw",
    "dataset": "production"
  },
  "project": {
    "name": "Sanity + Shopify demo"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/dashboard",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "media",
    "@sanity/color-input",
    "sanity-shopify-intro",
    "sanity-shopify-sync"
  ],
  "env": {
    "development": {
      "plugins": ["@sanity/vision"]
    }
